<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
  </head>
  <body>
    <table>
      <% for(const obj of data){ %>
      <tr>
        <% for(const key in obj) { %>
        <td><%= obj[key] %></td>
        <% } %>
        <td type="button"><a href="/edit-crud?id=<%=obj.id%>">Edit</a></td>
        <td type="button"><a href="/delete-crud?id=<%=obj.id%>">Delete</a></td>
      </tr>
      <% } %>
    </table>
  </body>
</html>

<style>
  table {
    width: 60%;
    border-collapse: collapse;
  }
  th,
  td {
    border: 1px solid black;
    padding: 8px;
    text-align: left;
  }
  th {
    background-color: #f2f2f2;
  }
</style>
